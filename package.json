{
  "name": "wasm-by-example",
  "version": "0.0.1",
  "description": "A website for Examples in how to do common things in WebAssembly",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "npx npm-run-all build --parallel serve watch",
    "build": "node build.js",
    "serve": "npx http-server ./dist",
    "watch": "npx watch 'npm run build' ./examples ./shell build.js --interval=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torch2424/wasm-by-example.git"
  },
  "keywords": [
    "WebAssembly",
    "Wasm",
    "Examples",
    "Example",
    "Documentation",
    "Demo",
    "Learning"
  ],
  "author": "Aaron Turner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/torch2424/wasm-by-example/issues"
  },
  "homepage": "https://github.com/torch2424/wasm-by-example#readme",
  "devDependencies": {
    "highlight.js": "^9.15.6",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "mustache": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "recursive-readdir": "^2.2.2",
    "watch": "^1.0.2"
  }
}
